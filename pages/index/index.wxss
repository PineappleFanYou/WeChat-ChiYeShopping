.index_swiper swiper {
  height: 340rpx;
}
.category {
  display: flex;
}
.category navigator {
  flex: 1;
  margin: 30rpx;
}
.floor_content .product_img {
  padding: 20rpx;
}
.floor_content .product_img .product_list {
  width: 33.33333%;
  float: left;
}
.floor_content .product_img .product_list image {
  height: 207.97413793rpx;
  display: block;
  border-right: 10rpx #fff solid;
}
.floor_content .product_img .product_list:first-child image {
  height: 415.94827586rpx;
}
.floor_content .product_img .product_list:nth-child(4) image {
  border-top: 10rpx #fff solid;
}
.floor_content .product_img .product_list:nth-child(5) image {
  border-top: 10rpx #fff solid;
}
.clearfix::after {
  content: '';
  clear: both;
  display: block;
  visibility: hidden;
  height: 0;
  line-height: 0;
}
.floor_title image {
  background-color: #f4f4f4;
}
